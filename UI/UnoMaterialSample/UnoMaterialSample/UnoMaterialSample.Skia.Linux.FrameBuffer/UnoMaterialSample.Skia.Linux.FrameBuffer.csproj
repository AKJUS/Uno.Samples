<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType Condition="'$(Configuration)'=='Release'">WinExe</OutputType>
    <OutputType Condition="'$(Configuration)'=='Debug'">Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup Condition="exists('..\UnoMaterialSample.Windows')">
    <EmbeddedResource Include="..\UnoMaterialSample.Windows\Package.appxmanifest" LogicalName="Package.appxmanifest" />
    <Content Include="..\UnoMaterialSample.Windows\Images\StoreLogo.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="7.0.0" />
    <PackageReference Include="Uno.WinUI.Skia.Linux.FrameBuffer" Version="5.0.64" />
    <PackageReference Include="Uno.WinUI.DevServer" Version="5.0.64" Condition="'$(Configuration)'=='Debug'" />
    <PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="5.0.64" />
    <PackageReference Include="Uno.WinUI.Lottie" Version="5.0.64" />
    <PackageReference Include="SkiaSharp.Views.Uno.WinUI" Version="2.88.6" />
    <PackageReference Include="SkiaSharp.Skottie" Version="2.88.6" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\UnoMaterialSample\UnoMaterialSample.csproj" />
  </ItemGroup>
  <Import Project="..\UnoMaterialSample.Base\base.props" />
</Project>