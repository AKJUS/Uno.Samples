<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0-ios</TargetFramework>
	<SingleProject>true</SingleProject> 	  
    <OutputType>Exe</OutputType>
	<RuntimeIdentifier>iossimulator-x64</RuntimeIdentifier>  
    <Nullable>enable</Nullable>
    <ImplicitUsings>true</ImplicitUsings>
    <SupportedOSPlatformVersion>13.0</SupportedOSPlatformVersion>
	<UseInterpreter Condition="'$(Configuration)' == 'Debug'">True</UseInterpreter>
	<IsUnoHead>true</IsUnoHead>
  </PropertyGroup>
	<ItemGroup>
		<PackageReference Include="System.Memory" Version="4.5.5" />
		<PackageReference Include="Uno.Core" Version="4.0.1" />
		<PackageReference Include="Uno.Core.Extensions" Version="4.0.1" />
		<PackageReference Include="Uno.Core.Extensions.Logging" Version="4.0.1" />
		<PackageReference Include="Uno.Extensions" Version="2.2.0" />
		<PackageReference Include="Uno.Extensions.Logging.OSLog" />
		<PackageReference Include="Uno.SourceGenerationTasks" Version="4.1.0" />	
		<PackageReference Include="Uno.Toolkit.UI" Version="2.3.0" />
		<PackageReference Include="Uno.UI" Version="4.5.14" />
		<PackageReference Include="Uno.UI.RemoteControl" Version="4.5.14" Condition="'$(Configuration)'=='Debug'" />
		<PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="4.5.14" />
		<PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />		
		<PackageReference Include="Uno.XamlMerge.Task" Version="1.0.0" />
	</ItemGroup>
	
  <ItemGroup>	  
    <None Remove="Resources\Default-568h@2x.png" />
    <None Remove="Resources\Fonts\winjs-symbols.ttf" />
    <None Remove="Resources\SplashScreen@2x.png" />
    <None Remove="Resources\SplashScreen@3x.png" />
  </ItemGroup>
	
  <ItemGroup>
    <PackageReference Update="Uno.Extensions.Logging.OSLog" Version="1.4.0" />
  </ItemGroup>
  <Import Project="..\TabViewSample\TabViewSample.Shared\TabViewSample.Shared.projitems" Label="Shared" />
</Project>
