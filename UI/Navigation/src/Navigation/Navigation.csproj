<Project Sdk="Uno.Sdk">
  <PropertyGroup>
    <TargetFrameworks>
      net8.0-android;
      net8.0-ios;
      net8.0-maccatalyst;
      net8.0-windows10.0.19041;
      net8.0-desktop;
      net8.0-browserwasm;
    </TargetFrameworks>

    <OutputType>Exe</OutputType>
    <UnoSingleProject>true</UnoSingleProject>

    <!-- Display name -->
    <ApplicationTitle>Navigation</ApplicationTitle>
    <!-- App Identifier -->
    <ApplicationId>com.companyname.Navigation</ApplicationId>
    <!-- Versions -->
    <ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
    <ApplicationVersion>1</ApplicationVersion>

    <UnoFeatures>
      Material;
      Hosting;
      Toolkit;
      Mvvm;
      Navigation;
      Extensions;
    </UnoFeatures>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Presentation\MessageDialog\MessageDialogPage.xaml" />
  </ItemGroup>
  <ItemGroup>
    <UpToDateCheckInput Remove="Presentation\MessageDialog\MessageDialogPage.xaml" />
  </ItemGroup>
  <ItemGroup>
    <Page Update="Presentation\MessageDialog\MessageDialogPage.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
  </ItemGroup>
</Project>
